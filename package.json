{
  "name": "next-boilerplate",
  "version": "0.0.0",
  "private": false,
  "author": {
    "name": "Mehrdad Dehghanzadeh",
    "email": "mehrdad.dehghanzadeh.dev@gmail.com"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mehrdad-Dehghanzadeh/next-boilerplate.git"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,jsx,css,md,tsx,ts}": "prettier --write"
  },
  "scripts": {
    "dev": "env-cmd -e dev -r .env.json next dev",
    "build": "env-cmd -e prod -r .env.json next build",
    "start": "env-cmd -e prod -r .env.json next start",
    "lint": "env-cmd -e dev -r .env.json next lint",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material-nextjs": "^5.15.11",
    "@mui/x-data-grid": "^7.3.0",
    "@mui/x-date-pickers": "^6.19.9",
    "axios": "^1.6.8",
    "cookies-next": "^4.1.1",
    "date-fns": "^3.6.0",
    "date-fns-jalali": "^2.13.0-0",
    "env-cmd": "^10.1.0",
    "jwt-decode": "^4.0.0",
    "next": "^14.2.3",
    "next-intl": "^3.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "stylis": "^4.3.2",
    "stylis-plugin-rtl": "^2.1.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/stylis": "^4.2.5",
    "autoprefixer": "^10.4.19",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "3.1.1",
    "sass": "^1.75.0",
    "typescript": "^5.4.5"
  }
}
