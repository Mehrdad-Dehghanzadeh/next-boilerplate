{
  "name": "next-boilerplate",
  "version": "0.0.0",
  "private": false,
  "author": {
    "name": "Mehrdad Dehghanzadeh",
    "email": "mehrdad.dehghanzadeh.dev@gmail.com"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mehrdad-Dehghanzadeh/next-boilerplate.git"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,jsx,css,md,tsx,ts}": "prettier --write"
  },
  "scripts": {
    "dev": "env-cmd -e dev -r .env.json next dev",
    "build": "env-cmd -e prod -r .env.json next build",
    "start": "env-cmd -e prod -r .env.json next start",
    "lint": "env-cmd -e dev -r .env.json next lint",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.14",
    "@mui/material-nextjs": "^5.16.14",
    "@mui/x-data-grid": "^7.27.0",
    "@mui/x-date-pickers": "^6.20.2",
    "axios": "^1.7.9",
    "cookies-next": "^4.3.0",
    "date-fns": "^3.6.0",
    "date-fns-jalali": "^2.13.0-0",
    "env-cmd": "^10.1.0",
    "jwt-decode": "^4.0.0",
    "next": "^14.2.24",
    "next-intl": "^3.26.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "stylis": "^4.3.6",
    "stylis-plugin-rtl": "^2.1.1",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@types/node": "^20.17.19",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/stylis": "^4.2.7",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "prettier": "3.1.1",
    "sass": "^1.85.0",
    "typescript": "^5.7.3"
  }
}
