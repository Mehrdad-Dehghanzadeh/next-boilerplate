{
  "name": "next-boilerplate",
  "version": "0.0.0",
  "private": false,
  "author": {
    "name": "Mehrdad Dehghanzadeh",
    "email": "mehrdad.dehghanzadeh.dev@gmail.com"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mehrdad-Dehghanzadeh/next-boilerplate.git"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "dev": "env-cmd -e dev -r .env.json next dev",
    "build": "env-cmd -e prod -r .env.json next build",
    "start": "env-cmd -e prod -r .env.json next start",
    "lint": "env-cmd -e dev -r .env.json next lint",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "env-cmd": "^10.1.0",
    "next": "14.1.0",
    "next-intl": "^3.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.15",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.1",
    "prettier": "3.1.1",
    "typescript": "^5.3.3"
  }
}
